{
  "name": "adhdwebapp-assets",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "css:build": "postcss wwwroot/css/dashboard.css -o wwwroot/css/dashboard.min.css",
    "css:watch": "postcss wwwroot/css/dashboard.css -o wwwroot/css/dashboard.min.css --watch",
    "css:purge:report": "purgecss --css wwwroot/css/dashboard.css --content \"Views/**/*.cshtml\" \"wwwroot/js/**/*.js\" --rejected --out temp/purge"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "cssnano": "^6.0.1",
    "purgecss": "^5.0.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.0"
  }
}
